import java.io.IOException;

public class SwapString {
    public static void main(String[] args) throws IOException{
        String str="I love Programming";
        char[] ch=str.toCharArray();
        int start=0,end=str.length()-1;
        while (start<=end){
            char temp=ch[start];
            ch[start]=ch[end];
            ch[end]=temp;
            start++;
            end--;
        }
        for (char c:ch             ) {
            System.out.print(c);
        }
    }
}

