#include <stdio.h> 
int len(char arr[]);

int main() {
	int len(char arr[])
	{
    	int i=0;
    	int c=0;
    	for(i=0;arr[i]!='\0';i++){
    		c++;
    	}
    	return c;
    }
	char str[100];
	scanf("%[^\n]",str);
	int i=0;
	int j=len(str)-1;
	while(i<j){
		int temp=str[i];
		str[i]=str[j];
		str[j]=temp;
		i++;j--;
	}
    printf("%s",str);
// 	for(i=0;i<len(str);i++){
		
// 	}
	return 0; 
	
}
